<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="anchor" prefHeight="960.0" prefWidth="1080.0" styleClass="jchat-anchor" stylesheets="@app.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.jchat.DashboardController">
    <children>
        <Button layoutX="14.0" layoutY="920.0" mnemonicParsing="false" onMouseClicked="#onReturnToDashboardClick" styleClass="jchat-dash-button" stylesheets="@app.css" text="Return To Dashboard" />
        <Label layoutX="92.0" styleClass="jchat-label" stylesheets="@app.css" text="Ongoing Chat Rooms" />
        <ScrollPane blendMode="OVERLAY" layoutX="41.0" layoutY="114.0" prefHeight="792.0" prefWidth="998.0">
            <content>
                <Label prefHeight="128.0" prefWidth="997.0" text="Displays all open chat rooms and actives users in room" textFill="RED">
                    <font>
                        <Font size="22.0" />
                    </font>
               <graphic>
                  <ButtonBar prefHeight="42.0" prefWidth="231.0" stylesheets="@app.css">
                    <buttons>
                        <Button mnemonicParsing="false" prefHeight="31.0" prefWidth="163.0" stylesheets="@app.css" text="Close Room">
                           <cursor>
                              <Cursor fx:constant="OPEN_HAND" />
                           </cursor>
                        </Button>
                      <Button mnemonicParsing="false" stylesheets="@app.css" text="Join Room">
                           <cursor>
                              <Cursor fx:constant="OPEN_HAND" />
                           </cursor>
                        </Button>
                    </buttons>
                  </ButtonBar>
               </graphic>
                </Label>
            </content>
        </ScrollPane>
    </children>
</AnchorPane>
